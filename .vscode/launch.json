    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/main.go",
            "envFile": "${workspaceFolder}/.env",
            "env": {
                "CGO_CFLAGS":"-I/opt/homebrew/include",
                "CGO_LDFLAGS":"-g -O2 -L/opt/homebrew/lib"
            }
        }

    ]
}